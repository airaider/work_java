package com.ssafy.java;

public class AlphaTest1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char a='A';
		int b;
		b=a;
		for(int i=0;i<5;i++) {
			for(int j=1;j<=i+1;j++) {
				System.out.print((char)(b));
				b=b+j;
			}
			System.out.println();
		}
	}

}
