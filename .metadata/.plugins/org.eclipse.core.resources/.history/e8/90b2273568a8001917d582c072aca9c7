package com.ssafy.java;

public class AlphaTest2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char a='A';
		int cnt=0;
		for(int i=5;i>=1;i--) {
			for(int j=1;j<i;j++) {
				System.out.print(" ");
			}
			for(int j=5;j>=i;j--) {
				System.out.print((char)(a+cnt)+" ");
				cnt++;
			}
			System.out.println();
		}
	}

}
