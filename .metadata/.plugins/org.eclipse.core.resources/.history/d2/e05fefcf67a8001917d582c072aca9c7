package com.ssafy.java;

public class AlphaTest1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char a='A';
		int b;
		int cnt=0;
		for(int i=0;i<5;i++) {
			for(int j=0;j<=i;j++) {
				System.out.print((char)(a+cnt)+" ");
				cnt++;
				
			}
			System.out.println();
		}
	}

}
