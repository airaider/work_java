package chapter03;

import java.util.Scanner;

public class whileTest {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n;
		int sum=0;
		int count=0;
		while(true) {
			n=scan.nextInt();
			if(n==100) {
				sum+=n;
				++count;
				break;
			}
			else {
				sum+=n;
				++count;
			}
		}
		double avg=sum/count;
		System.out.println(sum);
		System.out.println(Math.round(avg)/10);
	}

}
